<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>TargetDomestic</TeamProject><Wiql>SELECT [System.Id], [System.Title], [System.State], [Microsoft.VSTS.Common.StateChangeDate], [Retalix.CustomTemplate.ActualEffort], [Retalix.CustomTemplate.BugType], [Microsoft.VSTS.Common.ResolvedDate], [Retalix.CustomTemplate.AgreedRelease], [System.CreatedDate] FROM WorkItems WHERE [System.TeamProject] = @project  AND  [System.WorkItemType] = 'Bug'  AND  [Retalix.CustomTemplate.DetectedRelease] CONTAINS '5'  AND  [System.State] IN ('Fixed', 'passed qa', 'pending qa', 'closed') ORDER BY [System.State] desc </Wiql></WorkItemQuery>