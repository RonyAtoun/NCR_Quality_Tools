<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>R10</TeamProject><Wiql>SELECT [System.Id], [System.Title], [System.State], [System.CreatedDate], [Microsoft.VSTS.Common.StateChangeDate], [Retalix.CustomTemplate.BugType], [Retalix.CustomTemplate.AgreedRelease], [Microsoft.VSTS.Common.ResolvedDate], [Retalix.CustomTemplate.ActualEffort], [System.Reason], [Retalix.CustomTemplate.QAResolution] FROM WorkItems WHERE [System.TeamProject] = @project  AND  [System.WorkItemType] = 'Bug'  AND  [Retalix.CustomTemplate.DetectedRelease] CONTAINS '10.5'  AND  [System.State] IN ('Fixed', 'Passed QA') ORDER BY [System.State] </Wiql></WorkItemQuery>