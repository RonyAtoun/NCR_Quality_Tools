<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>WholeFoods</TeamProject><Wiql>SELECT [System.Id], [System.Title], [Retalix.CustomTemplate.SoftwareDomain], [System.State], [Microsoft.VSTS.Common.StateChangeDate], [Retalix.CustomTemplate.ActualEffort], [Retalix.CustomTemplate.BugType], [Microsoft.VSTS.Common.ResolvedDate], [Retalix.CustomTemplate.AgreedRelease], [System.CreatedDate] FROM WorkItems WHERE [System.TeamProject] = @project and [System.WorkItemType] = 'Bug' and [Retalix.CustomTemplate.DetectedRelease] contains '3.0' and [Retalix.CustomTemplate.DetectedRelease] &lt;&gt; '3.0.5' ORDER BY [Retalix.CustomTemplate.DetectedRelease] desc </Wiql></WorkItemQuery>