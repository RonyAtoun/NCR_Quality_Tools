<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>Coles</TeamProject><Wiql>SELECT [System.Id], [System.Title], [System.State], [Microsoft.VSTS.Common.StateChangeDate], [Retalix.CustomTemplate.ActualEffort], [Retalix.CustomTemplate.BugType], [Microsoft.VSTS.Common.ResolvedDate], [Retalix.CustomTemplate.AgreedRelease], [System.CreatedDate] FROM WorkItems WHERE [System.TeamProject] = @project and [System.WorkItemType] = 'Bug' and [Retalix.CustomTemplate.DetectedDrop] in ('01', '02', '03', '04') ORDER BY [Microsoft.VSTS.Common.StackRank], [Microsoft.VSTS.Common.Priority], [Microsoft.VSTS.Common.Severity], [System.Id] </Wiql></WorkItemQuery>