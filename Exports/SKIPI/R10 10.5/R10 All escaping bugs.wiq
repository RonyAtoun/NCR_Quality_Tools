<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>R10</TeamProject><Wiql>SELECT [System.Id], [System.Title], [System.State], [System.CreatedDate], [Microsoft.VSTS.Common.StateChangeDate], [Retalix.CustomTemplate.BugType], [Retalix.CustomTemplate.AgreedRelease], [Microsoft.VSTS.Common.ResolvedDate], [Retalix.CustomTemplate.ActualEffort] FROM WorkItems WHERE [System.TeamProject] = @project and [System.WorkItemType] = 'Bug' and [Retalix.CustomTemplate.DetectedRelease] contains '10.5' and [Retalix.CustomTemplate.CustomerName] &lt;&gt; 'Retalix' ORDER BY [System.CreatedDate] desc </Wiql></WorkItemQuery>