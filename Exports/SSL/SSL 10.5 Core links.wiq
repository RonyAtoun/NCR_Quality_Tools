<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>Sainsburys</TeamProject><Wiql>SELECT [System.Id], [System.Links.LinkType], [System.WorkItemType], [System.Title], [System.AssignedTo], [System.State], [System.Reason], [Retalix.CustomTemplate.QCRefNumber], [System.CreatedBy], [Microsoft.VSTS.Common.Severity] FROM WorkItemLinks WHERE ([Source].[System.TeamProject] = @project  AND  [Source].[System.WorkItemType] = 'Bug'  AND  [Source].[Retalix.CustomTemplate.DetectedRelease] IN ('4.0.0', '4.1.0', '4.1.1', '4.1.2', '4.3.0', '4.3.1', '4.3.2', '4.4.0', '4.4.1', '4.4.2', '4.5.0', '4.5.1', '4.5.2', '4.6.0')) And ([System.Links.LinkType] &lt;&gt; '') And ([Target].[System.WorkItemType] = 'Bug'  AND  [Target].[System.TeamProject] = 'R10') ORDER BY [Microsoft.VSTS.Common.Severity], [System.Id] mode(MustContain)</Wiql></WorkItemQuery>