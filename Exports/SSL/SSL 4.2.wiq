<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>Sainsburys</TeamProject><Wiql>SELECT [System.Id], [System.WorkItemType], [System.Title], [System.AssignedTo], [System.State], [Retalix.CustomTemplate.TeamLeadName], [Microsoft.VSTS.Common.Severity], [Microsoft.VSTS.Common.ResolvedDate], [Retalix.CustomTemplate.QCRefNumber], [Retalix.CustomTemplate.AgreedRelease], [System.CreatedDate] FROM WorkItems WHERE [System.TeamProject] = @project  AND  [System.WorkItemType] = 'Bug'  AND  [Retalix.CustomTemplate.DetectedRelease] &lt; '4.0.0'  AND  [System.CreatedDate] &gt;= '2013-01-05T00:00:00.0000000' ORDER BY [Retalix.CustomTemplate.TeamLeadName] desc </Wiql></WorkItemQuery>