<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>Sainsburys</TeamProject><Wiql>SELECT [System.Id], [System.Links.LinkType], [System.WorkItemType], [System.Title], [System.AssignedTo], [System.State], [Retalix.CustomTemplate.TeamLeadName], [Microsoft.VSTS.Common.Severity], [Microsoft.VSTS.Common.ResolvedDate], [Retalix.CustomTemplate.QCRefNumber], [Retalix.CustomTemplate.AgreedRelease], [System.CreatedDate] FROM WorkItemLinks WHERE ([Source].[System.TeamProject] = @project  AND  [Source].[System.WorkItemType] = 'Bug'  AND  [Source].[Retalix.CustomTemplate.DetectedRelease] &lt; '4.0.0'  AND  [Source].[System.CreatedDate] &gt;= '2013-01-05T00:00:00.0000000') And ([System.Links.LinkType] &lt;&gt; '') And ([Target].[System.WorkItemType] = 'Bug'  AND  [Target].[System.TeamProject] = 'R10') ORDER BY [Retalix.CustomTemplate.TeamLeadName] desc mode(MustContain)</Wiql></WorkItemQuery>