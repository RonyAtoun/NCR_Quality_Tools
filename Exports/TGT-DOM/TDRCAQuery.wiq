<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>TargetDomestic</TeamProject><Wiql>SELECT [System.Id], [System.Links.LinkType], [System.WorkItemType], [System.Title], [System.AssignedTo], [System.State], [Retalix.CustomTemplate.TeamLeadName] FROM WorkItemLinks WHERE ([Source].[System.TeamProject] = @project  AND  [Source].[System.WorkItemType] = 'Bug'  AND  [Source].[Microsoft.VSTS.Common.ResolvedBy] &lt;&gt; ''  AND  [Source].[Retalix.CustomTemplate.AgreedRelease] = '5.2.0') And ([System.Links.LinkType] &lt;&gt; '') And ([Target].[System.WorkItemType] = 'Bug'  AND  [Target].[Retalix.CustomTemplate.AgreedRelease] = '5.1.1') ORDER BY [Retalix.CustomTemplate.TeamLeadName] mode(DoesNotContain)</Wiql></WorkItemQuery>