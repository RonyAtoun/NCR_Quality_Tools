<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>TargetDomestic</TeamProject><Wiql>SELECT [System.Id], [System.Links.LinkType], [System.Title] FROM WorkItemLinks WHERE ([Source].[System.TeamProject] = @project  AND  [Source].[System.WorkItemType] = 'Bug'  AND  [Source].[Retalix.CustomTemplate.DetectedRelease] IN ('5.0.0', '5.0.1', '5.0.2', '5.0.3')) And ([System.Links.LinkType] &lt;&gt; '') And ([Target].[System.WorkItemType] = 'Bug'  AND  [Target].[System.TeamProject] = 'R10') ORDER BY [System.State] mode(MustContain)</Wiql></WorkItemQuery>