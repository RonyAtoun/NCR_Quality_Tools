<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>R10Projects</TeamProject><Wiql>SELECT [System.Id], [System.Links.LinkType], [System.WorkItemType], [System.Title], [System.AssignedTo], [System.State], [System.CreatedDate], [Retalix.CustomTemplate.AgreedRelease], [System.TeamProject] FROM WorkItemLinks WHERE ((( [Source].[System.TeamProject] = 'Coles'  OR  [Source].[System.TeamProject] = 'TargetCanada' ) OR ( [Source].[System.TeamProject] = 'Sainsburys'  AND  [Source].[Retalix.CustomTemplate.DetectedRelease] CONTAINS '4.'  AND  [Source].[Retalix.CustomTemplate.DetectedRelease] NOT CONTAINS '2.4.0'  AND  [Source].[Retalix.CustomTemplate.DetectedRelease] NOT CONTAINS '1.4.0'  AND  [Source].[Retalix.CustomTemplate.DetectedRelease] NOT CONTAINS '2.14' ) OR ( [Source].[System.TeamProject] = 'Tesco'  AND  [Source].[Retalix.CustomTemplate.AgreedRelease] CONTAINS '5.' ) OR  [Source].[System.TeamProject] = 'WholeFoods'  OR  [Source].[System.TeamProject] = 'Woolworths' ) AND  [Source].[System.WorkItemType] = 'Bug'  AND  [Source].[System.CreatedDate] &gt; @today - 365  AND  [Source].[System.State] &lt;&gt; 'cancelled') And ([System.Links.LinkType] &lt;&gt; '') And ([Target].[System.WorkItemType] = 'Bug'  AND  [Target].[System.TeamProject] = 'R10') ORDER BY [System.TeamProject] mode(MustContain)</Wiql></WorkItemQuery>