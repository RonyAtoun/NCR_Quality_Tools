<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>R10Projects</TeamProject><Wiql>SELECT [System.Id], [System.WorkItemType], [System.Title], [System.AssignedTo], [System.State], [System.CreatedDate], [Retalix.CustomTemplate.AgreedRelease], [System.TeamProject] FROM WorkItems WHERE (( [System.TeamProject] = 'Coles'  OR  [System.TeamProject] = 'TargetCanada' ) OR ( [System.TeamProject] = 'Sainsburys'  AND  [Retalix.CustomTemplate.DetectedRelease] CONTAINS '4.'  AND  [Retalix.CustomTemplate.DetectedRelease] NOT CONTAINS '2.4.0'  AND  [Retalix.CustomTemplate.DetectedRelease] NOT CONTAINS '1.4.0'  AND  [Retalix.CustomTemplate.DetectedRelease] NOT CONTAINS '2.14' ) OR ( [System.TeamProject] = 'Tesco'  AND  [Retalix.CustomTemplate.AgreedRelease] CONTAINS '5.' ) OR  [System.TeamProject] = 'WholeFoods'  OR  [System.TeamProject] = 'Woolworths' ) AND  [System.WorkItemType] = 'Bug'  AND  [System.CreatedDate] &gt; @today - 365  AND  [System.State] &lt;&gt; 'cancelled' ORDER BY [System.TeamProject] </Wiql></WorkItemQuery>