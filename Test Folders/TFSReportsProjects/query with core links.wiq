<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>TargetDomestic</TeamProject><Wiql>SELECT [System.Id], [System.Links.LinkType], [System.Title], [System.State] FROM WorkItemLinks WHERE ([Source].[System.TeamProject] = @project  AND  [Source].[System.WorkItemType] = 'Bug'  AND  [Source].[Retalix.CustomTemplate.DetectedRelease] CONTAINS '5'  AND  [Source].[System.CreatedDate] &gt;= @today - 30) And ([System.Links.LinkType] &lt;&gt; '') And ([Target].[System.WorkItemType] = 'Bug'  AND  [Target].[System.TeamProject] = 'R10') ORDER BY [System.Id] mode(MayContain)</Wiql></WorkItemQuery>