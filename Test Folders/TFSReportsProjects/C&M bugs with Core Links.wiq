<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>CustomerAndMarketing</TeamProject><Wiql>SELECT [System.Id], [System.Links.LinkType], [System.WorkItemType], [System.Title], [System.AssignedTo], [System.State] FROM WorkItemLinks WHERE ([Source].[System.TeamProject] = @project  AND  [Source].[System.WorkItemType] = 'Bug') And ([System.Links.LinkType] &lt;&gt; '') And ([Target].[System.WorkItemType] = 'Bug'  AND  [Target].[System.TeamProject] = 'R10') ORDER BY [System.Id] mode(MustContain)</Wiql></WorkItemQuery>