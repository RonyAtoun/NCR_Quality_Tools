<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>Sainsburys</TeamProject><Wiql>SELECT [System.Id], [System.Links.LinkType], [System.Title] FROM WorkItemLinks WHERE ([Source].[System.TeamProject] = @project  AND  [Source].[System.WorkItemType] = 'Bug'  AND  [Source].[Retalix.CustomTemplate.AgreedRelease] CONTAINS '2.'  AND  [Source].[System.CreatedDate] &gt; '2014-01-01T00:00:00.0000000') And ([System.Links.LinkType] &lt;&gt; '') And ([Target].[System.WorkItemType] = 'Bug'  AND  [Target].[System.TeamProject] = 'R10') ORDER BY [Retalix.CustomTemplate.AgreedRelease] mode(MustContain)</Wiql></WorkItemQuery>