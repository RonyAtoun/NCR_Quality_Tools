<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>WholeFoods</TeamProject><Wiql>SELECT [System.Id], [System.Links.LinkType], [System.WorkItemType], [System.Title], [Microsoft.VSTS.Common.Severity], [Retalix.CustomTemplate.TeamLeadName], [System.AssignedTo], [System.State], [Retalix.CustomTemplate.DetectedRelease], [Microsoft.VSTS.Common.Priority], [Retalix.CustomTemplate.AgreedRelease], [Retalix.CustomTemplate.RequestedRelease], [Retalix.CustomTemplate.QCRefNumber], [System.CreatedDate] FROM WorkItemLinks WHERE ([Source].[System.TeamProject] = @project  AND  [Source].[System.WorkItemType] = 'Bug'  AND  [Source].[Retalix.CustomTemplate.DetectedRelease] CONTAINS '3.0') And ([System.Links.LinkType] &lt;&gt; '') And ([Target].[System.WorkItemType] = 'Bug'  AND  [Target].[System.TeamProject] = 'R10') ORDER BY [Retalix.CustomTemplate.DetectedRelease] mode(MustContain)</Wiql></WorkItemQuery>