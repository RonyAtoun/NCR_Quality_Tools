<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>R10</TeamProject><Wiql>SELECT [System.Id], [System.Title], [System.State], [System.CreatedDate], [Microsoft.VSTS.Common.StateChangeDate], [Retalix.CustomTemplate.BugType], [Retalix.CustomTemplate.AgreedRelease], [Microsoft.VSTS.Common.ResolvedDate], [Retalix.CustomTemplate.ActualEffort], [System.Reason], [Retalix.CustomTemplate.QAResolution], [Retalix.CustomTemplate.TeamLeadName] FROM WorkItems WHERE [System.TeamProject] = @project  AND  [System.WorkItemType] = 'Bug'  AND ( [Retalix.CustomTemplate.AgreedRelease] CONTAINS '5.1.'  OR  [Retalix.CustomTemplate.AgreedRelease] = '5.1' ) AND  [System.CreatedDate] &gt;= @today - 30 ORDER BY [System.CreatedDate] desc </Wiql></WorkItemQuery>