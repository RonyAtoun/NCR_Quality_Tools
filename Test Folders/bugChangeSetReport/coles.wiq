<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>Coles</TeamProject><Wiql>SELECT [System.Id], [System.Title], [System.State], [Microsoft.VSTS.Common.StateChangeDate], [Retalix.CustomTemplate.ActualEffort], [Retalix.CustomTemplate.BugType], [Microsoft.VSTS.Common.ResolvedDate], [Retalix.CustomTemplate.AgreedRelease], [System.CreatedDate], [System.Reason], [Retalix.CustomTemplate.TeamLeadName], [Retalix.CustomTemplate.QAResolution] FROM WorkItems WHERE [System.TeamProject] = @project and [System.WorkItemType] = 'Bug' and [Retalix.CustomTemplate.DetectedRelease] = '5.0.0' and [System.CreatedDate] &gt; @today - 90 ORDER BY [System.CreatedDate] </Wiql></WorkItemQuery>