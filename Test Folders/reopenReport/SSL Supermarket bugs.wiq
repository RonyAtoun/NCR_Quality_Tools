<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>Sainsburys</TeamProject><Wiql>SELECT [System.Id], [System.Title], [Retalix.CustomTemplate.SoftwareDomain], [System.State], [Microsoft.VSTS.Common.StateChangeDate], [Retalix.CustomTemplate.ActualEffort], [Retalix.CustomTemplate.BugType], [Microsoft.VSTS.Common.ResolvedDate], [Retalix.CustomTemplate.AgreedRelease], [System.CreatedDate] FROM WorkItems WHERE [System.TeamProject] = @project  AND  [System.WorkItemType] = 'Bug'  AND  [Retalix.CustomTemplate.DetectedRelease] CONTAINS '4.'  AND  [Retalix.CustomTemplate.DetectedRelease] NOT CONTAINS '2.4.0'  AND  [Retalix.CustomTemplate.DetectedRelease] NOT CONTAINS '1.4.0'  AND  [Retalix.CustomTemplate.DetectedRelease] NOT CONTAINS '2.14' ORDER BY [Retalix.CustomTemplate.AgreedRelease] </Wiql></WorkItemQuery>