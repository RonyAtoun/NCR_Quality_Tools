<?xml version="1.0" encoding="utf-8"?><WorkItemQuery Version="1"><TeamFoundationServer>http://srraatfsapp:8080/tfs/defaultcollection</TeamFoundationServer><TeamProject>Woolworths</TeamProject><Wiql>SELECT [System.Id], [System.Links.LinkType], [System.Title], [System.State], [Microsoft.VSTS.Common.StateChangeDate], [Retalix.CustomTemplate.ActualEffort], [Retalix.CustomTemplate.BugType], [Microsoft.VSTS.Common.ResolvedDate], [Retalix.CustomTemplate.AgreedRelease], [System.CreatedDate] FROM WorkItemLinks WHERE ([Source].[System.TeamProject] = @project  AND  [Source].[System.WorkItemType] = 'Bug') And ([System.Links.LinkType] &lt;&gt; '') And ([Target].[System.WorkItemType] = 'Bug'  AND  [Target].[System.TeamProject] = 'R10') ORDER BY [System.State] mode(MustContain)</Wiql></WorkItemQuery>